<body>
	<div class="fondoUNMSM">.</div>

	<div class="centrar">
		<div class="row">
			<div class="col">
				<div class="informacion">
					<div class="login">
						<div class="logo">
							<img src="/img/tramites.png" alt="logo">
						</div>
						<h2 class="t-center">Inicio de Sesión</h2>


						<form action="/">
							<div class="inputWithIcon">
								<input type="email" id="email" placeholder="Tu correo" required>
								<i class="fa fa-user fa-lg fa-fw" aria-hidden="true"></i>
								<span class="underline"></span>
							</div>
							<div class="inputWithIcon">
								<input type="password" id="password" placeholder="Tu contraseña" required>
								<i class="fa fa-lock" aria-hidden="true"></i>
							</div>

							<button type="button" id="btnLogin" class="btn btn-danger">Ingresar</button>

						</form>
					</div>
					<div class="guia-usuario">
						<p class="t-center">
							Registrate haciendo click <a href="/registro"> aquí</a> </p>
					</div>
				</div>
			</div>
			<div class="col d-none">
				<img class="fondo-secundario" src="/img/admin.jpg" alt="">
			</div>
		</div>

	</div>

	<script src="https://kit.fontawesome.com/ee93dc3392.js" crossorigin="anonymous"></script>
